{
  "name": "tai11s",
  "version": "1.0.0",
  "description": "An 11ty starter with TailwindCSS, PostCSS, PurgeCSS and other modern tools",
  "main": "index.js",
  "scripts": {
    "data": "node build/fetchRecipes.js",
    "clean": "rm -rf dist",
    "eleventy:dev": "eleventy --serve 8080 --watch",
    "eleventy:default": "eleventy",
    "styles:prod": "postcss src/styles/*.css -o dist/styles/index.css --minify",
    "styles:dev": "postcss src/styles/*.css -o dist/styles/index.css",
    "scripts:prod": "webpack --mode=production",
    "scripts:dev": "webpack --mode=development",
    "serve": "light-server -c .lightserverrc",
    "dev": "NODE_ENV=development npm-run-all --parallel data clean scripts:dev eleventy:dev styles:dev  --print-label",
    "dev:debug": "DEBUG=* npm run dev",
    "build": "NODE_ENV=production run-s data clean scripts:prod eleventy:default styles:prod --print-label"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ianrose@github.com/ianrose/deventy.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianrose/deventy/issues"
  },
  "homepage": "https://github.com/ianrose/deventy#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "dotenv": "^10.0.0",
    "light-server": "^2.6.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.0.0",
    "recipe-scraper": "^1.25.0",
    "slugify": "^1.4.6",
    "tailwindcss": "^2.2.19",
    "to-title-case": "^1.0.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "acorn": "^6.4.2",
    "axios": "^0.23.0",
    "cheerio": "^1.0.0-rc.3",
    "node-fetch": "^3.0.0",
    "parse-domain": "^2.3.2",
    "postcss": "^8.3.11",
    "puppeteer": "^1.20.0",
    "request": "^2.88.0"
  }
}
